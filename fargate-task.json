{
  "containerDefinitions": [
        {
            "name": "app",
            "image": "kodekloud/ecs-project1",
            "portMappings": [
                { 
                    "containerPort": 3000,
                    "hostPort": 3000,
                    "protocol": "tcp"
                }
            ]
        }
    ],

    "family": "sample-fargate",
    "executionRoleArn": "arn:aws:iam::363254031734:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "volumes": [],
    "placementConstraints": [],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "512",
    "memory": "1024"
}
